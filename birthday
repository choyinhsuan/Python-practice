def birthday(s, d, m):
    # Write your code here
    ans = 0
    count = 0
    for i in range(m):
        count += s[i]
    length = len(s)-m+1

    for index in range(length):
        if count == d:
            ans += 1
        count -= s[index]
        if index+m != len(s):
            count += s[index+m]
    return(ans)
