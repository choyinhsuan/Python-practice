def birthday(s, d, m):
    # Write your code here
    ans = 0
    count = 0
    for i in range(m):
        count += s[i]
    length = len(s)-m
    if length <= 0:
        if length == 0 and s[0] == d:
            return(1)
        else:
            return(0)
    for index in range(length):
        if count == d:
            ans += 1
        count -= s[index]
        count += s[index+m]
    return(ans)
